-- Created by Jordan
-- Supports ModuleScript
-- Checks for FE
-- Mobile and PC support
-- Remastered version

local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
local screenGui = Instance.new("ScreenGui", playerGui)
screenGui.Name = "c00lkiddExecutor"
screenGui.ResetOnSpawn = false
screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Name = "Mainframe"
mainFrame.Size = UDim2.new(0, 640, 0, 310)
mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
mainFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
mainFrame.BackgroundTransparency = 0.5
mainFrame.BorderColor3 = Color3.fromRGB(255, 0, 0)
mainFrame.BorderSizePixel = 3
mainFrame.Active = true
mainFrame.Selectable = true

local UserInputService = game:GetService("UserInputService")
local dragging = false
local dragInput, mousePos, framePos

local function update(input)
    local delta = input.Position - mousePos
    mainFrame.Position = UDim2.new(
        framePos.X.Scale,
        framePos.X.Offset + delta.X,
        framePos.Y.Scale,
        framePos.Y.Offset + delta.Y
    )
end

mainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        mousePos = input.Position
        framePos = mainFrame.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

mainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)

local scale = Instance.new("UIScale", mainFrame)
if UserInputService.TouchEnabled then
    scale.Scale = 0.55
else
    scale.Scale = 1
end

local image = Instance.new("ImageLabel", mainFrame)
image.Size = UDim2.new(0, 240, 0, 226)
image.Position = UDim2.new(0.599, 0, -0.188, 0)
image.BackgroundTransparency = 1
image.Image = "rbxassetid://96083365637107"
image.Rotation = 14

local scrollFrame = Instance.new("ScrollingFrame", mainFrame)
scrollFrame.Size = UDim2.new(0, 611, 0, 227)
scrollFrame.Position = UDim2.new(0.022, 0, 0.133, 0)
scrollFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
scrollFrame.ScrollBarThickness = 8
scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 1000000)

local scriptBox = Instance.new("TextBox", scrollFrame)
scriptBox.Size = UDim2.new(1, 0, 1, 0)
scriptBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
scriptBox.TextColor3 = Color3.fromRGB(170, 170, 170)
scriptBox.Font = Enum.Font.SourceSans
scriptBox.TextSize = 14
scriptBox.TextXAlignment = Enum.TextXAlignment.Left
scriptBox.TextYAlignment = Enum.TextYAlignment.Top
scriptBox.ClearTextOnFocus = false
scriptBox.MultiLine = true
scriptBox.TextWrapped = true
scriptBox.TextEditable = true
scriptBox.Text = ""

local function createButton(name, text, position, size, parent)
    local btn = Instance.new("TextButton", parent)
    btn.Name = name
    btn.Text = text
    btn.Position = position
    btn.Size = size
    btn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    btn.BorderColor3 = Color3.fromRGB(154, 54, 54)
    btn.BorderSizePixel = 2
    btn.Font = Enum.Font.SourceSans
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.TextScaled = true
    btn.TextWrapped = true
    return btn
end

local autoFillBtn = createButton("AutoFill", "Auto Username Fill", UDim2.new(0.02, 0, 0.019, 0), UDim2.new(0, 137, 0, 28), mainFrame)
local executeBtn = createButton("Execute", "Execute", UDim2.new(0.02, 0, 0.887, 0), UDim2.new(0, 137, 0, 28), mainFrame)
local clearBtn = createButton("Clear", "Clear", UDim2.new(0.266, 0, 0.887, 0), UDim2.new(0, 137, 0, 28), mainFrame)
local resetBtn = createButton("Reset", "Reset Character", UDim2.new(0.64, 0, 0.887, 0), UDim2.new(0, 100, 0, 28), mainFrame)
local scriptTypeBtn = createButton("ScriptType", "Regular", UDim2.new(0.818, 0, 0.887, 0), UDim2.new(0, 100, 0, 28), mainFrame)

local label = Instance.new("TextLabel", mainFrame)
label.Size = UDim2.new(0, 200, 0, 40)
label.Position = UDim2.new(0.45, 0, 0, 0)
label.BackgroundTransparency = 1
label.Text = "c00lkidd Executor"
label.Font = Enum.Font.SourceSans
label.TextColor3 = Color3.fromRGB(255, 255, 255)
label.TextScaled = true
label.TextWrapped = true

clearBtn.MouseButton1Click:Connect(function()
    scriptBox.Text = ""
end)

resetBtn.MouseButton1Click:Connect(function()
    local humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.Health = 0
    end
end)

autoFillBtn.MouseButton1Click:Connect(function()
    scriptBox.Text = scriptBox.Text:gsub("%(%s*\"[^\"]-\"%s*%)", "(\"" .. game.Players.LocalPlayer.Name .. "\")")
end)

local scriptMode = "Regular"

scriptTypeBtn.MouseButton1Click:Connect(function()
    scriptMode = (scriptMode == "Regular") and "Module" or "Regular"
    scriptTypeBtn.Text = scriptMode
end)

local function getRemote()
	for _, v in pairs(game:GetDescendants()) do
		if v:IsA("RemoteEvent") then
			local n = v.Name:lower()
			if n:find("exec") 
			   or n:find("server") 
			   or n:find("backdoor") 
			   or n:find("load") 
			   or n:find("remote") 
			   or n:find("admin") 
			   or v.Name == "RemoteEvent" 
			   or v.Name == "Remote Event" then
				return v
			end
		end
	end
	return nil
end

executeBtn.MouseButton1Click:Connect(function()
    local code = scriptBox.Text
    if code == "" then return end

    if scriptMode == "Regular" then
        pcall(function()
            loadstring(code)()
        end)
    elseif scriptMode == "Module" then
        local remote = getRemote()
        if not remote then return end
        pcall(function()
            remote:FireServer(code)
        end)
    end
end)

local StarterGui = game:GetService("StarterGui")
local function showNotification(message)
	StarterGui:SetCore("SendNotification", {
		Title = "c00lkidd Executor";
		Text = message;
		Duration = 1.5;
	})
end
local isFilteringEnabled = game:GetService("Workspace").FilteringEnabled
if isFilteringEnabled then
	showNotification("Filtering Enabled (FE)")
else
	showNotification("Filtering Disabled (FD)")
end
